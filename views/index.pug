extends layout

block content
  h1= title
  p Welcome to #{title} Praneeth Kumar Vobilishetty
   
  script.
    var client_count = 0;
    function update(){
      client_count +=1;
      document.getElementById("target").innerHTML = "Value now " + client_count;
    }
  if (!user)
    a(href="/login") Login
    br
    a(href="/register") Register
  if (user)
    p You are currently logged in as #{user.username}
    a(href="/car") Take me to car
    br
    a(href="/logout") Logout